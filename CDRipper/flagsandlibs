
AM_CPPFLAGS =	-I$(top_srcdir) \
			$(GTHREAD_CFLAGS) \
			$(GDK_CFLAGS) \
			$(GLIB_CFLAGS) \
			$(CDDB_CFLAGS) \
			$(GTK_CFLAGS) \
